{
  "name": "eyra-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build src && tsc --project tsconfig.server.json",
    "dev": "ts-node --compiler-options '{\"module\":\"commonjs\"}' server.ts",
    "start": "ts-node --compiler-options '{\"module\":\"commonjs\"}' server.ts",
    "start:production": "NODE_ENV=production node dist/server.js",
    "test": "react-scripts test --env=jsdom",
    "lint": "tslint --project .",
    "cypress-install": "yarn add --peer @cypress/webpack-preprocessor@3.0.1 cypress@3.1.4",
    "cypress": "cypress open",
    "cypress-test": "react-scripts start & wait-on http://localhost:3000 && cypress run",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@babel/core": "^7.1.2",
    "@material-ui/core": "^4.4.0",
    "@material-ui/icons": "^4.2.0",
    "@material-ui/styles": "^4.1.1",
    "@nteract/markdown": "^4.0.0",
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "express": "^4.17.1",
    "font-awesome": "^4.7.0",
    "formik": "^1.5.7",
    "moment": "^2.24.0",
    "next": "^8.1.0",
    "nprogress": "^0.2.0",
    "postcss": "^7.0.17",
    "postcss-clean": "^1.1.0",
    "prismic-javascript": "^2.0.3",
    "prismic-reactjs": "^1.0.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-popout": "^1.0.2",
    "react-rte": "^0.16.1",
    "react-transition-group": "^4.1.1",
    "styled-jsx": "^3.2.1",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.8",
    "@types/clean-css": "^4.2.1",
    "@types/express": "^4.17.0",
    "@types/jest": "^24.0.15",
    "@types/next": "^8.0.5",
    "@types/node": "^12.7.3",
    "@types/nprogress": "^0.2.0",
    "@types/react": "16.8.20",
    "@types/react-dom": "16.9.0",
    "@types/react-rte": "^0.16.0",
    "@types/react-transition-group": "^2.9.2",
    "@types/styled-jsx": "^2.2.8",
    "@types/yup": "^0.26.16",
    "@zeit/next-css": "1.0.1",
    "@zeit/next-typescript": "^1.1.1",
    "autoprefixer": "^9.6.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "clean-css": "^4.2.1",
    "file-loader": "^4.0.0",
    "husky": "^3.0.5",
    "lint-staged": "^8.2.1",
    "next-routes": "^1.4.2",
    "ts-loader": "^6.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "4.0.0",
    "typescript": "3.6.2",
    "typescript-plugin-css-modules": "^1.2.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.ts",
      "src/**/*.tsx",
      "src/**/*.js",
      "src/**/*.jsx",
      "!src/index.tsx"
    ]
  },
  "lint-staged": {
    "linters": {
      "src/*.{ts,tsx}": [
        "yarn lint",
        "git add"
      ]
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
