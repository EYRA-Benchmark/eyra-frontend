{
  "name": "eyra-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next",
    "build": "next build src",
    "start": "next start src",
    "test": "react-scripts test --env=jsdom",
    "lint": "tslint --project .",
    "cypress-install": "yarn add --peer @cypress/webpack-preprocessor@3.0.1 cypress@3.1.4",
    "cypress": "cypress open",
    "cypress-test": "react-scripts start & wait-on http://localhost:3000 && cypress run",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@material-ui/core": "next",
    "@material-ui/icons": "next",
    "@material-ui/styles": "next",
    "@nteract/markdown": "^4.0.0",
    "@types/nprogress": "^0.0.29",
    "@types/styled-jsx": "^2.2.8",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "express": "^4.16.4",
    "font-awesome": "^4.7.0",
    "formik": "^1.5.1",
    "next": "^8.0.4",
    "nprogress": "^0.2.0",
    "prismic-javascript": "^2.0.1",
    "prismic-reactjs": "^0.3.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-rte": "^0.16.1",
    "react-transition-group": "^2.7.1",
    "styled-jsx": "^3.2.1",
    "yup": "^0.26.10"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.7",
    "@types/jest": "^24.0.11",
    "@types/next": "^8.0.4",
    "@types/node": "^11.13.0",
    "@types/react": "16.8.10",
    "@types/react-dom": "16.8.3",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-rte": "^0.16.0",
    "@types/react-transition-group": "^2.0.16",
    "@types/yup": "^0.26.12",
    "@zeit/next-css": "1.0.1",
    "@zeit/next-typescript": "^1.1.1",
    "babel-plugin-module-resolver": "^3.2.0",
    "file-loader": "^3.0.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "ts-loader": "^5.3.3",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "3.6.0",
    "typescript": "3.4.1",
    "typescript-plugin-css-modules": "^1.0.5",
    "wait-on": "^3.1.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.ts",
      "src/**/*.tsx",
      "src/**/*.js",
      "src/**/*.jsx",
      "!src/index.tsx"
    ]
  },
  "lint-staged": {
    "linters": {
      "*.{ts,tsx}": [
        "yarn lint",
        "git add"
      ]
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
