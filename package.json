{
  "name": "eyra-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "lint": "tslint --project .",
    "cypress-install": "yarn add --peer @cypress/webpack-preprocessor@3.0.1 cypress@3.1.4",
    "cypress": "cypress open",
    "cypress-test": "react-scripts start & wait-on http://localhost:3000 && cypress run",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "@nteract/markdown": "^4.0.0",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "font-awesome": "^4.7.0",
    "formik": "^1.5.1",
    "prismic-javascript": "^2.0.1",
    "prismic-reactjs": "^0.3.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-rte": "^0.16.1",
    "react-transition-group": "^2.7.1",
    "yup": "^0.26.10"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.7",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.13.0",
    "@types/react": "16.8.17",
    "@types/react-dom": "16.8.3",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-rte": "^0.16.0",
    "@types/react-transition-group": "^2.0.16",
    "@types/yup": "^0.26.12",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "react-scripts": "2.1.8",
    "ts-loader": "^5.3.3",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "3.6.0",
    "typescript": "3.4.1",
    "typescript-plugin-css-modules": "^1.0.5",
    "wait-on": "^3.1.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.ts",
      "src/**/*.tsx",
      "src/**/*.js",
      "src/**/*.jsx",
      "!src/index.tsx"
    ]
  },
  "lint-staged": {
    "linters": {
      "*.{ts,tsx}": [
        "yarn lint",
        "git add"
      ]
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
